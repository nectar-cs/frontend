{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/sort-comp": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "react/require-default-props": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-props-no-spreading": "off",
    "react/no-array-index-key": "off",
    "react/static-property-placement": "off",
    "no-unused-expressions": "off",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-shadow": "off", // todo - warn later
    "no-param-reassign": "off", // todo - warn later
    "react/forbid-prop-type": "off",
    "no-unused-vars": "off", // todo - warn later
    "no-console": "off",
    "react/no-unescaped-entities": "off",
    "react/forbid-prop-types": "off",
    "react/no-unused-prop-types": "off",
    "react/prefer-stateless-function": "off", // todo -warn later
    "no-undef": "off", // todo - warn later
    "react/button-has-type": "off", // todo - warn later
    "class-methods-use-this": "off", // todo - warn later
    "prefer-destructuring": "off", // todo - warn later
    "react/jsx-one-expression-per-line": "off",
    "max-classes-per-file": "off",
    "consistent-return": "off", // todo - warn later
    "react/jsx-fragments": "off",
    "react/jsx-curly-brace-presence": "off",
    "import/order": "off",
    "spaced-comment": "off",
    "dot-notation": "off",
    "prefer-template": "off"
  }
}
